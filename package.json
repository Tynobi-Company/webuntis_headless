{
    "name": "webuntis_headless",
    "version": "0.0.0",
    "description": "WebUntis Headless API",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.mjs",
    "scripts": {
        "build": "tsup index.ts --format cjs,esm --dts",
        "lint": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tynobi-Company/webuntis_headless.git"
    },
    "keywords": [
        "tynopia",
        "untis",
        "webuntis",
        "school",
        "teacher",
        "student"
    ],
    "author": "Lukas Leisten / Joel Obi",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Tynobi-Company/webuntis_headless/issues"
    },
    "homepage": "https://github.com/Tynobi-Company/webuntis_headless#readme",
    "devDependencies": {
        "ts-node": "^10.9.2",
        "tsup": "^8.0.1",
        "typescript": "^5.3.2"
    },
    "dependencies": {
        "axios": "^1.6.2"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "tag": "latest"
    },
    "release": {
        "branches": [
            "main"
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/github",
            "@semantic-release/npm"
        ]
    }
}